<h2>Courses:</h2>

<table class="classlist">
  <tr>
    <th>Title</th>
    <th>Professor</th>
  </tr>

<% @courses.each do |course| %>
  <tr>
    <td><%= link_to course.title, course %></td>
    <td><%= course.professor %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Course', new_course_path %>
